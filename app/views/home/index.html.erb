<div class="row-fluid">
	<div class="span12">
		<div id="home-title">
			Join a Networking Group	
		</div>	
	</div>
</div>


<div class="row-fluid">
	<div class="span12">
		<div id="home-networking_group-buttons">
<% @groups.each do |group| %>	
			<%= link_to group_path(group), :class => "home-networking_group-buttons-css" do %>
			<%= group.name %>
			<% end %>
		</div>	
	</div>
</div>

<% end %>